<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="298.0" prefWidth="468.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Archivo">
          <items>
            <MenuItem fx:id="miSalir" mnemonicParsing="false" onAction="#salir" text="Salir" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Ayuda">
          <items>
            <MenuItem mnemonicParsing="false" text="Ayuda" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="294.0" prefWidth="558.0" VBox.vgrow="ALWAYS">
      <children>
            <Label layoutX="93.0" layoutY="21.0" text="Accede con tu usuario y contraseña">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="121.0" layoutY="73.0" text="Usuario" />
            <TextField fx:id="tfUsuario" layoutX="231.0" layoutY="69.0" />
            <Label layoutX="121.0" layoutY="136.0" text="Contraseña" />
            <PasswordField fx:id="pfContrasena" layoutX="231.0" layoutY="132.0" />
            <Button fx:id="btAceptar" layoutX="203.0" layoutY="191.0" mnemonicParsing="false" onAction="#aceptarUsuario" text="Aceptar" />
      </children>
    </AnchorPane>
  </children>
</VBox>
